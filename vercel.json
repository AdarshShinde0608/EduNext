{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install"
  
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/edunext/api/[...route].js" },
    { "handle": "filesystem" },
    { "src": "^(?!/api/).*", "dest": "/index.html" }
  ]
}

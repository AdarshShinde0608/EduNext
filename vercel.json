{
  "version": 2,
  "buildCommand": "cd edunext && npm run build",
  "outputDirectory": "edunext/dist",
  "installCommand": "cd edunext && npm install",
  "routes": [
    {
      "src": "/api/test",
      "dest": "/edunext/api/test.js"
    },
    {
      "src": "/api/health",
      "dest": "/edunext/api/health.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/edunext/api/[...route].js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

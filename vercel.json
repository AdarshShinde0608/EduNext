{
  "version": 2,
  "buildCommand": "cd edunext && npm run build",
  "outputDirectory": "edunext/dist",
  "installCommand": "cd edunext && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...route].js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/edunext/dist/index.html"
    }
  ]
}
